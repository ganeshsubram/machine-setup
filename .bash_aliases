# System
alias refresh='source ~/.bashrc'
alias cue='code ~/.bash_aliases'
alias machine='cd ~/machine-setup'
alias code_machine='code ~/machine-setup'
# File system
alias exp='xdg-open .'
alias ll='ls -alF'
alias la='ls -A'
alias lf='ls -CF'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../../..'
alias .....='cd ../../../../..'
alias ......='cd ../../../../../..'
alias .......='cd ../../../../../../..'
# Git
alias gb='git branch'
alias gd='git diff'
alias gs='git status'
alias gp='git push'
alias gpf='git push -f'
alias gco='git checkout master'
alias gcom='git checkout main'
alias gpom='git pull origin main'
alias gal='git add --all'
alias grbi='git rebase -i main'
alias gcr='git rebase --continue'
alias cane='git commit --amend --no-edit'
# Tmux
alias tmux_refresh='tmux source-file ~/.tmux.conf'
# Glydways
alias ark='cd $HOME/glyd/third_party/ark'
alias at128='cd $HOME/glyd/glyd/onboard/lidar/hesai/at128'
alias at128test='$HOME/glyd/make.sh --run glyd/onboard/lidar/hesai/at128/all'
alias b1onalpha='cd $HOME/glyd/glyd/onboard/pipelines/b1_on_alpha'
alias b1onalphainit='$HOME/glyd/make.sh --run glyd_b1_on_alpha_data_collection -- --exit-after-initialize'
alias b1onalphaplayback='$HOME/glyd/make.sh --run glyd_b1_on_alpha_playback'
alias b1onalphaplaybackinit='$HOME/glyd/make.sh --run glyd_b1_on_alpha_playback -- --exit-after-initialize'
alias b1onalpharun='$HOME/glyd/make.sh --run glyd_b1_on_alpha_data_collection'
alias bench='cd $HOME/glyd/glyd/onboard/pipelines/bench'
alias benchinitrun='$HOME/glyd/make.sh --run glyd_bench_data_collection -- --exit-after-initialize'
alias benchplaybackinitrun='$HOME/glyd/make.sh --run glyd_bench_playback -- --exit-after-initialize'
alias benchplayback='$HOME/glyd/make.sh --run glyd_bench_playback'
alias benchrun='$HOME/glyd/make.sh --run glyd_bench_data_collection'
alias conti='cd $HOME/glyd/glyd/onboard/radar/continental/ars540'
alias contitest='$HOME/glyd/make.sh --run glyd/onboard/radar/continental/ars540/all'
alias gld='cd $HOME/glyd'
alias gldcode='code $HOME/glyd'
alias gldmake='$HOME/glyd/make.sh'
alias gui='$HOME/glyd/make.sh --run glyd_onboard_gui'
alias hesai='cd $HOME/glyd/glyd/onboard/lidar/hesai'
alias hesaitest='$HOME/glyd/make.sh --run glyd/onboard/lidar/hesai/all'
alias imu='cd $HOME/glyd/glyd/onboard/imu'
alias imutest='$HOME/glyd/make.sh --run glyd/onboard/imu/all'
alias lidar='cd $HOME/glyd/glyd/onboard/lidar'
alias lidartest='$HOME/glyd/make.sh --run glyd/onboard/lidar/all'
alias lint='cd $HOME/glyd && ./lint.sh && cd -'
alias linttidy='cd $HOME/glyd && ./lint.sh --lint-tidy && cd -'
alias ncom='cd $HOME/glyd/third_party/oxts_ncom_sdk'
alias onboardtest='$HOME/glyd/make.sh --run glyd/onboard/all'
alias oxts='cd $HOME/glyd/glyd/onboard/imu/oxts/ncom'
alias oxtstest='$HOME/glyd/make.sh --run glyd/onboard/imu/oxts/ncom/all'
alias radartest='$HOME/glyd/make.sh --run glyd/onboard/radar/all'
alias velodyne='cd $HOME/glyd/glyd/onboard/lidar/velodyne_v2'
alias velodynetest='$HOME/glyd/make.sh --run glyd/onboard/lidar/velodyne_v2/all'
alias xsens='cd $HOME/glyd/glyd/onboard/imu/xsens/mti'
alias xsenstest='$HOME/glyd/make.sh --run glyd/onboard/imu/xsens/mti/all'
alias dock1=' ssh -L 8080:localhost:8080 -L 8081:localhost:8081 glydways@gdf-ops-laptop-1 -t ssh -L 8080:localhost:8080 -L 8081:localhost:8081 arkuser@192.168.0.11'
alias dock2=' ssh -L 8080:localhost:8080 -L 8081:localhost:8081 glydways@gdf-ops-laptop-2 -t ssh -L 8080:localhost:8080 -L 8081:localhost:8081 arkuser@192.168.0.11'
alias deploy='./make.sh --run ark-supervisor-tool -- --host localhost:8081 --deploy-archive /tmp/glyd_b1_on_alpha_autonomy_release.tar.zst'
# Docker
alias docker_enable='systemctl --user enable docker-desktop'
alias docker_start='systemctl --user start docker-desktop'
alias docker_stop='systemctl --user stop docker-desktop'
